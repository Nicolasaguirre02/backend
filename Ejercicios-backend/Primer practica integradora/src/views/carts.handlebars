<h1>Carrito</h1>
<div>
   {{products}}  
    {{#each products}}
    <div>
        <div class="card border-primary mb-3" style="max-width: 25rem;">
            <div class="card-header">{{this.product}}</div>
            <div class="card-body text-primary">
                <h5 class="card-title">$ {{product.price}}</h5>
                <div class="row">
                    <div class="col-6">
                        <p class="card-text">Stock: {{product.disponible}}</p>
                        <p class="card-text">ID del Producto: {{product._id}}</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    {{/each}}
</div>
